    "eslint-plugin-jest": "^26.5.3",
    "jest": "^28.1.3",
    "js-yaml": "^4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "license": "MIT",
  "main": "lib/main.js",
